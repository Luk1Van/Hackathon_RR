@using Hackathon_RR.Models.HomeModels;
@model Dictionary<string, OutputModel>;

<table class="table">
    <thead>
        <tr>           
            <th scope="col">N локомотива</th>
            <th scope="col">Маршрут</th>
            <th scope="col">Расписание</th>
            <th scope="col">Вагоны</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var kvp in Model)
        {
            <tr>
                <td>@kvp.Key</td>
                <td>@string.Join(", ", kvp.Value.route)</td>
                <td>@string.Join("<br>", kvp.Value.timetable)</td>
                <td>@string.Join(", ", kvp.Value.vagons)</td>
            </tr>
        }
    </tbody>
</table>